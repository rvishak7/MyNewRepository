public class Printing1to100 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		int num[] = {0,1,2,3};
		for(int stud: num)
		System.out.println("Number : " + num);
		{
			for(int i=0; i<100; i++)
			System.out.println(num[i]+1);
		}}
			
		
	

}
